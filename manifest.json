{
  "manifest_version":3,
  "version":"0.0.1",
  "name":"GCalMakeFreeTransparent",
  "content_scripts":[
    {
     "matches":["*://calendar.google.com/*"],
     "js":["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icon.png"
  }
 }